        ],
        "settings": {
          "foreground": "#DCD7BA"
        }
      },
      {
        "name": "Markdown - Fenced Language",
        "scope": ["variable.language.fenced.markdown"],
        "settings": {
          "foreground": "#727169"
        }
      },
      {
        "name": "Markdown - Separator",
        "scope": ["meta.separator"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#9CABCA"
        }
      },
      {
        "name": "Markup - Table",
        "scope": ["markup.table"],
        "settings": {
          "foreground": "#DCD7BA"
        }
      },
      {
        "scope": [
          "comment",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type.js",
          "entity.other.attribute-name"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword.control.conditional", "keyword.operator"],
        "settings": {
          "fontStyle": ""
        }
      }
