{
  "breadcrumbs.enabled": false,
  "codeium.enableCodeLens": false,
  "codeium.enableSearch": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "biomejs.biome",
  "editor.fontFamily": "Rec Mono Linear",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.formatOnPaste": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  "editor.mouseWheelScrollSensitivity": 3,
  "editor.guides.highlightActiveIndentation": false,
  "editor.inlayHints.enabled": "off",
  "editor.inlineSuggest.showToolbar": "onHover",
  "editor.matchBrackets": "never",
  "editor.minimap.autohide": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": "off",
  "editor.renderLineHighlight": "gutter",
  "editor.scrollbar.vertical": "hidden",
  "editor.selectionHighlight": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.trimAutoWhitespace": true,
  "editor.wordWrapColumn": 80,
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "errorLens.gutterIconsEnabled": false,
  "errorLens.removeLinebreaks": true,
  "errorLens.scrollbarHackEnabled": true,
  "explorer.compactFolders": false, 
  "files.associations": {
    "*.css": "tailwindcss",
    "settings.json": "json"
  },
  "[json]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.stickyScroll.defaultModel": "foldingProviderModel"
  },
  "files.hotExit": "off",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "underline": "#FFA066"
      }
    }
  },
  "html.format.wrapLineLength": 80,
  "htmltagwrap.tag": "div",
  "javascript.suggest.paths": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "path-intellisense.showHiddenFiles": true,
  "relativePath.ignore": [
    "**/.git/**",
    "**/node_modules/**",
    "**/.husky/**",
    "**/.next/**",
    "**/*.dll",
    "**/*.swp",
    "**/*.un~",
    "**/obj/**",
    "**/objd/**",
    "**/src-tauri/target/**"
  ],
  "search.quickOpen.includeHistory": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": [
    "ms-vscode-remote.remote-wsl",
    "drcika.apc-extension"
  ],
  "settingsSync.ignoredSettings": [
    "editor.mouseWheelScrollSensitivity",
    "editor.fontFamily",
    "apc.electron",
    "apc.imports",
    "apc.stylesheet"
  ],
  "template-string-converter.addBracketsToProps": true,
  "template-string-converter.autoRemoveTemplateString": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "terminal.integrated.fontSize": 14,

  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "typescript.suggest.paths": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabSizingFixedMaxWidth": 150,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.activityBar.location": "top",
  "files.simpleDialog.enable": true,
  "window.menuBarVisibility": "compact",
  "explorer.confirmDragAndDrop": false,
  "editor.quickSuggestionsDelay": 1,
  "editor.suggest.preview": true,
  "apc.electron": {
    "titleBarStyle": "hidden",
    "titleBarOverlay": {
      "color": "#18181b",
      "symbolColor": "white",
      "height": 30
    }
  },
  
  "apc.font.family": "Recursive Sans Linear Static",
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 35,
    "itemMargin": 7
  },
  "apc.listRow": {
    "parts": {
      "quick-input-list": {
        "fontSize": 13,
        "height": 30
      },
      "explorer-folders-view": {
        "height": 25,
        "fontSize": 14
      },
      "tree": {
        "fontSize": 14,
        "height": 30
      }
    }
  },
  "apc.sidebar.titlebar": {
    "height": 35,
    "fontSize": 13
  },
  "apc.menubar.compact": true,
  "apc.imports": [
    "/C:/Users/zachb/Dotfiles/Global/VSCode/styles.css"
  ],
  "workbench.editor.pinnedTabSizing": "compact",
  "editor.hideCursorInOverviewRuler": true,
  "scm.diffDecorationsGutterVisibility": "hover",
  "scm.diffDecorationsGutterAction": "none",
  "scm.diffDecorations": "minimap",
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "window.dialogStyle": "custom",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json"
  },
  "workbench.tree.renderIndentGuides": "always",
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 18,
  "editor.guides.indentation": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome-nightly",
    "editor.stickyScroll.defaultModel": "foldingProviderModel"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.statusBar.visible": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "window.titleBarStyle": "native",
  "rapidapi.terminalLink.enabled": false,
  "workbench.colorTheme": "Ronin",
  "workbench.tree.enableStickyScroll": true,
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community"
}